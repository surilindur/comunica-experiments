{
  "name": "ltqp-solid-default",
  "private": true,
  "dependencies": {
    "solidbench": "^2.0.0",
    "sparql-benchmark-runner": "^5.0.0"
  },
  "scripts": {
    "compose:create": "docker compose create",
    "compose:start": "docker compose start",
    "compose:stop": "docker compose stop",
    "dataset:create": "yarn solidbench generate --cwd $(pwd)/generated --enhancementConfig ../input/solidbench/enhancer.json --queryConfig ../input/solidbench/queries.json --validationConfig ../input/solidbench/validation.json --fragmentConfig ../input/solidbench/fragmenter-${SOLIDBENCH_CONFIG}.json",
    "queries:execute": "yarn sparql-benchmark-runner --endpoint http://localhost:3000/sparql --queries ./generated/out-queries --replication 1 --warmup 0 --timeout 40 --output ./output/query-times-${COMUNICA_CONFIG}-${SOLIDBENCH_CONFIG}.csv"
  }
}
