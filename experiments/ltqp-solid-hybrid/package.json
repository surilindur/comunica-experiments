{
  "name": "ltqp-solid-hybrid",
  "private": true,
  "dependencies": {
    "solidbench": "^2.0.0",
    "sparql-benchmark-runner": "4.3.0"
  },
  "scripts": {
    "compose:create": "docker compose create",
    "compose:start": "docker compose start",
    "compose:stop": "docker compose stop",
    "dataset:create": "yarn run dataset:generate && yarn run dataset:postprocess",
    "dataset:generate": "yarn solidbench generate --cwd $(pwd)/generated --enhancementConfig ../input/solidbench/enhancer.json --queryConfig ../input/solidbench/queries.json --validationConfig ../input/solidbench/validation.json --fragmentConfig ../input/solidbench/fragmenter-${SOLIDBENCH_CONFIG}.json",
    "dataset:postprocess": "rename '.nt' '.meta' ./generated/out-fragments/**/.nt",
    "queries:execute": "yarn sparql-benchmark-runner --endpoint http://localhost:3000/sparql --queries ./generated/out-queries --replication 3 --warmup 0 --timeout 70 --output ./output/query-times-${COMUNICA_CONFIG}-${SOLIDBENCH_CONFIG}.csv",
    "queries:prepare": "rm ./generated/out-queries/interactive-complex-*"
  }
}
